{
	"name": "vault-app",
	"version": "1.0.1",
	"description": "Offline Vault App with Google Drive JSON sync",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"package-mac": "electron-packager . VaultApp --platform=darwin --arch=x64 --overwrite",
		"package-win": "electron-packager . VaultApp --platform=win32 --arch=ia32 --overwrite",
		"test": "jest"
	},
	"devDependencies": {
		"electron": "25.0.0",
		"electron-packager": "17.1.2",
		"jest": "29.0.0",
		"jest-environment-jsdom": "30.2.0"
	},
	"jest": {
		"testEnvironment": "jsdom"
	}
}
